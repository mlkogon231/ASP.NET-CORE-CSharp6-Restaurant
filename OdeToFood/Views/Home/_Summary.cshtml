@model Restaurant

<section class="panel panel-default">
    <h3 class="panel-heading">@Model.Name</h3>
    <div class="panel-body">
        @Model.Cuisine
    </div>
    <div class="panel-footer">
        <a class="btn btn-primary" asp-action="Details" asp-route-id="@Model.Id">More</a>
        <a class="btn btn-primary" asp-page="/Restaurants/Edit" asp-route-id="@Model.Id">Edit</a>
    </div><p></p>
</section>
